<%= simple_form_for(@business_profile) do |f| %>
  <%= f.error_notification %>

  <div class="form-group">
    <%= f.input :name, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :description, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :url, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :twitter, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :facebook, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :google, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :employees, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :slug, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :address, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :hiring, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :image, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :email, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :phone, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :linkedin, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :github, input_html: { class: 'form-control'} %>
  </div>
    <%= f.button :submit %>
<% end %>
